include ../components/headerProducts
include ../components/footer
include ../components/productsLeftBlock
include ../components/productsRightBlock
include ../components/requestCall
include ../components/navigation

doctype html
html(lang="en")

head
  meta(charset="UTF-8")
  meta(name="viewport" content="width=device-width, initial-scale=1.0")
  title Products

body
- let elemMenu = ['Компания', 'Видеоблог', 'Выполненные проекты', 'Бетон', 'Аренда бетононасоса', 'Лаборатория', 'Отзывы', 'Контакты']

+headerProducts(elemMenu)

main
  +menu(elemMenu)
  +navigation
  .wrap-main-blocks
    +productsLeftBlock()
    +productsRightBlock()


  +requestCall()

+footer()
